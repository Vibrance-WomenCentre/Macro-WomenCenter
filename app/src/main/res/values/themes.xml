<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.WomenCenter" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Base.Theme.MedEdukasi" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="colorPrimary">@color/Color_Primary</item>
    </style>

    <style name="Theme.MedEdukasi" parent="Base.Theme.MedEdukasi">
        <item name="fontFamily">@font/nunito_bold</item>
    </style>

    <style name="hiTitle">
        <item name="fontFamily">@font/nunito_medium</item>
    </style>

    <style name="NewsFont">
        <item name="fontFamily">@font/nunito_regular</item>
    </style>
</resources>