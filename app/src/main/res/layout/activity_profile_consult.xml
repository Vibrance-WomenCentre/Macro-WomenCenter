      android:fontFamily="@font/nunitoreguler"
      android:paddingStart="5dp"
      android:paddingEnd="5dp"
      android:text="@string/jumat"
      android:textColor="#7C7C7C"
      android:textSize="9.5sp"
      tools:layout_editor_absoluteX="146dp"
      tools:layout_editor_absoluteY="496dp" />

  <Button
      android:layout_width="56dp"
      android:layout_height="90dp"
      android:layout_marginLeft="10dp"
      android:layout_marginTop="5dp"
      android:fontFamily="@font/nunitoreguler"
      android:paddingStart="5dp"
      android:paddingEnd="5dp"
      android:text="@string/sabtu"
      android:textSize="9.5sp"
      tools:layout_editor_absoluteX="210dp"
      tools:layout_editor_absoluteY="496dp" />

  <Button
      android:layout_width="56dp"
      android:layout_height="90dp"
      android:layout_marginLeft="10dp"
      android:layout_marginTop="5dp"
      android:backgroundTint="@color/primary"
      android:fontFamily="@font/nunitoreguler"
      android:paddingStart="5dp"
      android:paddingEnd="5dp"
      android:text="@string/minggu"
      android:textColor="#7C7C7C"
      android:textSize="9.5sp"
      tools:layout_editor_absoluteX="279dp"
      tools:layout_editor_absoluteY="496dp" />

  <Button
      android:layout_width="55dp"
      android:layout_height="90dp"
      android:layout_marginLeft="10dp"
      android:layout_marginTop="5dp"
      android:backgroundTint="@color/primary"
      android:fontFamily="@font/nunitoreguler"
      android:paddingStart="5dp"
      android:paddingEnd="5dp"
      android:text="@string/senin"
      android:textColor="#7C7C7C"
      android:textSize="9.5sp"
      tools:layout_editor_absoluteX="340dp"
      tools:layout_editor_absoluteY="496dp"></Button>

 </LinearLayout>

 <TextView
     android:id="@+id/tv_selectTime"
     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:layout_marginBottom="120dp"
     android:fontFamily="@font/nuntobold"
     android:text="Pilih Waktu"
     android:layout_marginTop="10dp"
     android:layout_marginStart="20dp"
     app:layout_constraintTop_toBottomOf="@id/LayoutHari"
