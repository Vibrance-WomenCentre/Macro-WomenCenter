     android:layout_width="145dp"
     android:layout_height="47dp"
     android:backgroundTint="@color/pink"
     android:fontFamily="@font/nuntobold"
     android:paddingStart="5dp"
     android:paddingEnd="5dp"
     android:text="Pilih Jadwal"
